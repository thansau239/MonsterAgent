{
    "index_patterns": "logs-endpoint-monster-*",
    "settings": {
        "index.refresh_interval": "5s"
    },
    "mappings": {
        "doc": {
            "dynamic_templates": [{
                "strings": {
                    "match_mapping_type": "string",
                    "mapping": {
                        "type": "text",
                        "norms": false,
                        "fields": {
                            "raw": {
                                "type": "keyword",
                                "ignore_above": 256
                            }
                        }
                    }
                }
            }],
            "properties": {
                "@timestamp": {
                    "type": "date"
                },
                "@CollectTime": {
                    "type": "date"
                },
                "@ImageCreateTime": {
                    "type": "date"
                },
                "@ImageModifiedTime": {
                    "type": "date"
                },
                "@DisConnectTime": {
                    "type": "date"
                },
                "EventBlockKey": {
                    "type": "long"
                },
                "EventID": {
                    "type": "long"
                },
                "EventTimestamp(UTC)": {
                    "type": "long"
                },
                "ProcessId": {
                    "type": "long"
                },
                "ParentProcessId": {
                    "type": "long"
                },
                "TerminalSessionId": {
                    "type": "long"
                },
                "ClientIP": {
                    "type": "ip"
                },
                "ClientPort": {
                    "type": "long"
                },
                "SeverIP": {
                    "type": "ip"
                },
                "ServerPort": {
                    "type": "long"
                },
                "WriteCounts": {
                    "type": "long"
                },
                "WriteBytes": {
                    "type": "long"
                },
                "SendCount": {
                    "type": "long"
                },
                "SendBytes": {
                    "type": "long"
                },
                "RecvCount": {
                    "type": "long"
                },
                "RecvBytes": {
                    "type": "long"
                },
                "geoip": {
                    "properties": {
                        "dma_code": {
                            "type": "integer"
                        },
                        "latitude": {
                            "type": "float"
                        },
                        "location": {
                            "type": "geo_point"
                        },
                        "longitude": {
                            "type": "float"
                        }
                    }
                },
                "Group": {
                    "properties": {
                        "name": {
                            "type": "keyword"
                        },
                        "sid": {
                            "type": "keyword"
                        },
                        "attributes": {
                            "type": "long"
                        }
                    }
                },
                "Privilege": {
                    "properties": {
                        "name": {
                            "type": "keyword"
                        },
                        "attributes": {
                            "type": "long"
                        }
                    }
                },
                "HostIP": {
                    "type": "ip"
                }
            }
        }
    }
}
